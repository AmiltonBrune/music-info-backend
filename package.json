  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:down": "npx sequelize-cli db:migrate:undo:all",
    "db:seed:specific": "npx sequelize-cli db:seed --seed",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:undo:seed:all": "npx sequelize-cli db:seed:undo:all",
    "generate:migrate": "npx sequelize-cli migration:generate --name",
    "generate:seed": "npx sequelize-cli seed:generate --name",
    "test": "npx jest --runInBand"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.17",
    "express-validator": "^6.12.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^27.0.6",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.30.0",
    "jest": "^27.0.6",
    "jsinspect": "^0.12.7",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.3.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.2.0",
    "standard": "^16.0.3"
  }
